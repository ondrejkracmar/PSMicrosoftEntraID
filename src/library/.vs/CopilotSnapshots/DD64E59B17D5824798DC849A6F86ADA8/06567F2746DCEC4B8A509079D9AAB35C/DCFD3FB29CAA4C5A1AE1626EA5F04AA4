    {
        [DataMember(Name = "id")]
        public string Id { get; set; }

        [DataMember(Name = "deletedDateTime")]
        public DateTimeOffset? DeletedDateTime { get; set; }

        // Add additional properties as needed based on your use case or Graph API schema
    }
}